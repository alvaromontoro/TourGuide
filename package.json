{
  "name": "tourguide",
  "version": "0.0.2",
  "description": "JS plugin that helps generate step-by-step instructions and demos in an easy way",
  "directories": {
    "doc": "docs",
    "example": "examples",
    "test": "tests"
  },
  "main": "index.js",
  "scripts": {
    "prebuild": "node-sass --include-path scss src/tourguide.scss dist/tourguide.css",
    "prebuild:dev": "node-sass --include-path scss src/tourguide.scss dist/tourguide.css",
    "build": "webpack --output-filename tourguide.min.js --mode production",
    "build:dev": "webpack --output-filename tourguide.js --mode development",
    "test": "jest --coverage"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/alvaromontoro/tourguide.git"
  },
  "keywords": [
    "tourguide",
    "guided tour",
    "spotlight",
    "tutorial",
    "javascript",
    "js",
    "html",
    "css",
    "javascript-plugin",
    "highlight",
    "instructions",
    "demos",
    "step-by-step"
  ],
  "author": "Alvaro Montoro",
  "license": "MIT",
  "homepage": "https://github.com/alvaromontoro/tourguide",
  "dependencies": {},
  "devDependencies": {
    "@babel/cli": "^7.1.2",
    "@babel/core": "^7.1.2",
    "@babel/plugin-proposal-class-properties": "^7.1.0",
    "@babel/plugin-transform-modules-commonjs": "^7.1.0",
    "@babel/preset-env": "^7.1.0",
    "acorn": "^7.0.0",
    "babel-loader": "^8.0.4",
    "babel-plugin-add-module-exports": "^1.0.0",
    "babel-plugin-istanbul": "^5.0.1",
    "babel-plugin-transform-export-extensions": "^6.22.0",
    "babel-polyfill": "^6.26.0",
    "eslint": "^5.6.1",
    "eslint-loader": "^2.1.1",
    "jest": "^24.9.0",
    "node-sass": "^4.12.0",
    "nodemon": "^1.19.1",
    "webpack": "^4.39.2",
    "webpack-cli": "^3.3.7"
  },
  "bugs": {
    "url": "https://github.com/alvaromontoro/tourguide/issues"
  }
}
